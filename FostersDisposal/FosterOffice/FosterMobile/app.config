            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <FosterMobile.My.MySettings>
            <setting name="SelectedRouteDay" serializeAs="String">
