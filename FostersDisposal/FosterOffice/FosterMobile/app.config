            </setting>
            <setting name="SelectedRouteNumber" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="LastDatabaseDownload" serializeAs="String">
                <value />
            </setting>
            <setting name="DatabaseLocation" serializeAs="String">
                <value />
            </setting>
        </FosterMobile.My.MySettings>
    </userSettings>
</configuration>
